    <div id="mydiv"></div>
</body>
<script>
    
    function showNoOfSites(){
        var count=0;
        var arr=["www.google.com","www.amaxon.co.in","www.msm.com","in.answers,yahoo.com"];
        for(temp of arr){
            if(temp.startsWith("www")){
                count++;
            }
        }

        document.getElementById("mydiv").innerHTML="No of sites starting with www are "+count;
        
    }

</script>
</html>

 
