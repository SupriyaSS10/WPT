Problem 24:
<html>

<head>
    <script>
        function validateName() {
            var nm = document.getElementById("uname").value;
            if (nm.length >= 6 && nm.length <= 10) {
                document.getElementById("td1").innerHTML = "";
                return true;
            } else {
                document.getElementById("td1").innerHTML = "Username";
                return false;
            }
        }

        function validatePass() {
            var ps = document.getElementById("pass").value;
            var passPattern = /^(?=.*\d)(?=.*[A-Z])(.{6,50})$/;
            if (ps.match(passPattern)) {
                document.getElementById("td1").innerHTML = "";
                return true;
            }
            else {
                document.getElementById("td2").innerHTML = "Password should contains alteast one number and one capital character.";
                return false;
            }
        }

        function validate() {
            var nameflag = validateName();
            var passflag = validatePass();
            if (nameflag && passflag) {
                console.log("true");
                document.getElementById("success").innerHTML = "Logged in Successfully";
            }
        }
    </script>
</head>

<body>
    <form>
        <table>
            <tr>
                <td>
                    Username : <input type="text" name="uname" id="uname">
                </td>
                <td id="td1"></td>
            </tr>
            <tr>
                <td>
                    Password : <input type="password" name="pass" id="pass">
                </td>
                <td id="td2"></td>
            </tr>
        </table>
        <button type="button" name="btn" id="btn1" onclick="validate()">Submit</button>
        <p id="success"></p>
    </form>

</body>

</html>

 
