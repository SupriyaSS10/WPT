      Click me
    </button>
    <div id="mydiv"></div>
  </body>
  <script>
    var arr = ["Shruti", "Suraj", "Sakshi", "Prajwal"];
    var revArr = [];
    function showReverse() {
      for (var temp of arr) {
        revArr.push(temp.split("").reverse().join(""));
      }

      document.getElementById("mydiv").innerHTML = revArr.toString();
    }
  </script>
</html>


 
