Javascript
Day-1: JS Fundamental
Problem-1:
<html>

<head>
    <!-- <meta name="viewport" content="width=device-width, initial-scale=1.0"> -->
    <script>
        function largest() {
            // alert("Hello");
            var n1 = parseInt(document.getElementById("n1").value);
            var n2 = parseInt(document.getElementById("n2").value);
            var n3 = parseInt(document.getElementById("n3").value);
            if ((n1 > n2) && (n1 > n3)) {
                document.getElementById("mydiv").innerHTML += "the largest number is:" + n1;
            }
            else if ((n2 > n1) && (n2 > n3)) {
                document.getElementById("mydiv").innerHTML += "the largest number is:" + n2;
            }
            else {
                document.getElementById("mydiv").innerHTML += "the largest number is:" + n3;
            }
        }
    </script>
</head>

<body>

    num1:<input type="number" name="n1" id="n1">
    num2:<input type="number" name="n2" id="n2">
    num3:<input type="number" name="n3" id="n3">
    <button type="button" name="btn" id="btn" onclick="largest()">Submitt</button>
    <div id="mydiv">
        Answer:
    </div>
</body>

</html>


 
-----------------------------------------------------------------------------------------------
Problem-2:
<html>
<script>
    function submit() {
        var n = parseInt(document.getElementById("num").value);
        var msgs = document.getElementById("msg").value;

               for(var i=0;i<n;i++){
            document.getElementById("mydiv").innerHTML+=msgs+"<br>";

        }

    }
</script>

<body>
    num1:<input type="number" name="num" id="num">
    message: <input type="text" name="message" id="msg">
    <button type="button" name="btn" id="btn" onclick="submit()">submit</button>
    <div id="mydiv">Answer:</div>
    
</body>

</html>

 
Problem-3:
<html>
<script>
    function submit(){  
          var n = parseInt(document.getElementById("num").value)
    var i = 2;
    while ((i <= n)) {
        document.getElementById("mydiv").innerHTML +=" "+ i 
        i = i + 2;
    }
}
</script>

<body>
    num:<input type="number" name="num" id="num">
    <button type="button" name="btn" id="btn" onclick="submit()">submit</button>
    <div id="mydiv">Answer</div>
</body>

</html>
 
Problem-4:
<html>
    <script>
        function calculate(ev){
            alert("Hello");
            var n1=parseInt(document.getElementById("n1").value);
            var n2=parseInt(document.getElementById("n2").value);
            var arr=document.getElementsByName("operation");
            for(var i=0;i<arr.length;i++){
                if(arr[i].checked){  //checked which button is selected
                    var op=arr[i].value; //get value of that button
                }
            }

            var v=document.getElementById("mydiv");
            switch(op){
                case "+":
                    var ans=n1+n2;
                    v.innerHTML=`Addition ${n1+n2}`;
                    break;
                case "-":
                    var ans=n1-n2;
                    v.innerHTML=`Subtraction ${n1-n2}`;
                    break;
                case "/":
                    var ans=n1/n2;
                    v.innerHTML+=`division ${n1/n2}`;
                    break;
                case "*":
                    var ans=n1*n2;
                    v.innerHTML=`Multiplication ${n1*n2}`;
                    break;
            }
            document.getElementById("n1").value="";
            document.getElementById("n2").value="";
            document.getElementById("result").value=ans;

        }
    </script>
    <body>
        <form>
            num1: <input type="text" name="n1" id="n1">
            num2: <input type="text" name="n2" id="n2">
             Result: <input type="text" name="result" id="result" readonly> <!--readonly used in result here so we only read the value in that box can not change or modify value in that box-->
            <input type="radio" name="operation" id="btn" value="+"><label for="btn">addition</label></button>
            <input type="radio" name="operation" id="btn1" value="-"><label for="btn1">subtraction</label></button>
            <input type="radio" name="operation" id="btn2" value="/"><label for="btn2">division</label></button>
            <input type="radio" name="operation" id="btn3" value="*"><label for="btn3">multiplication</label></button>
            <button type="button" name="btn" id="btn4" onclick="calculate()">calculate</button> <!-- type="button" used when data required to display on same page-->
        </form>
        <div id="mydiv">
           Answer: 
        </div>
    </body>
</html>

 
 
