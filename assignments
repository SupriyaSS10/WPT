    <script>
        var arr = ["Prashant", "Akshay", "Eknath", "Chaitanya", "Vinayak"];
        function sortedArray() {
            document.getElementById("mydiv").innerHTML = " Array before sorting :-  <br>";
            for (var a of arr) {
                document.getElementById("mydiv").innerHTML += a + "  ";
            }
            document.getElementById("mydiv").innerHTML += "<br/> Array after sorting :-  <br>";
            arr.sort();
            for (var a of arr) {
                document.getElementById("mydiv").innerHTML += a + "  ";
            }
        }

    </script>
