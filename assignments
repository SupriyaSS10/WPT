Problem-13:
<html>

<head>
    <script>
        var empArr = [{ empid: 11, empname: "Prashant", empsalary: 50000 }];
        function submit() {
            var empid = document.getElementById("id").value;
            var empname = document.getElementById("name").value;
            var empsalary = document.getElementById("sal").value;

            obj = { empid, empname, empsalary };

            empArr.push(obj);
            document.getElementById("id").value = "";
            document.getElementById("name").value = "";
            document.getElementById("sal").value = "";
            display();
        }
        function display(arr = empArr) {

            str = `<table border="2">`;
            str += `<tr><td>ID</td><td>NAME</td><td>SALARY</td></tr>`;
            for (var a of arr) {
                str += `<tr><td>${a.empid}</td><td>${a.empname}</td><td>${a.empsalary}</td></tr>`;
            }
            str += `</table>`;
            document.getElementById("mydiv").innerHTML = str;
        }
    </script>
</head>

<body>
    Emp Id : <input type="text" name="empid" id="id">
    Emp Name : <input type="text" name="empname" id="name">
    Emp Salary : <input type="text" name="salary" id="sal">
    <button type="button" name="btn11" id="btn1" onclick="submit()">Submit</button>
    <button type="button" name="btn22" id="btn2" onclick="display()">Display Table</button>
    <div id="mydiv"></div>

</body>

</html>

 
 

