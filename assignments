Problem-18
<html>

<head>
    <script>
        function getresult() {
            var radioarr = document.getElementsByName("rad");
            for (i = 0; i < radioarr.length; i++) {
                if (radioarr[i].checked) {

                    var op = radioarr[i].value;

                }
            }
            switch (op) {
                case "r1":
                    var ostr = document.getElementById("os1").value;
                    var substr = document.getElementById("os2").value;
                    if (ostr.includes(substr)) {
                        alert("string found !!");
                    } else {
                        alert("string not found !!");
                    }
                    break;

                case "r2":
                    var ostr = document.getElementById("os1").value;

                    alert(ostr.toUpperCase());
                    break;

                case "r3":
                    var ostr = document.getElementById("os1").value;

                    alert(ostr.toLowerCase());
                    break;

                case "r4":
                    var ostr = document.getElementById("os1").value;
                    var tostr = ostr.trim();
                    alert(tostr);
                    document.getElementById("os1").value = tostr;
                    break;

                case "r5":
                    var ostr = document.getElementById("os1").value;
                    var substr = document.getElementById("os2").value;
                    alert(ostr.concat(substr));
                    break;

                case "r6":
                    var ostr = document.getElementById("os1").value;
                    var repstr = document.getElementById("os3").value;
                    alert(repstr);
                    document.getElementById("os1").value = repstr;
                    break;
            }
        }
    </script>

</head>

<body>
    Enter String : <input type="text" name="ostring" id="os1" size="15" maxlength="20" autofocus><br>
    <p></p>
    Enter Sub String : <input type="text" name="ostring" id="os2" size="15" maxlength="20"><br>
    <p></p>
    Replace with String : <input type="text" name="ostring" id="os3" size="15" maxlength="20"><br>
    <p></p>
    <input type="radio" name="rad" id="r1" value="r1"><label for="r1">Search substring in main string</label>
    <input type="radio" name="rad" id="r2" value="r2"><label for="r2">To Upper Case</label>
    <input type="radio" name="rad" id="r3" value="r3"><label for="r3">To Lower Case</label>
    <input type="radio" name="rad" id="r4" value="r4"><label for="r4">Trim spaces</label>
    <input type="radio" name="rad" id="r5" value="r5"><label for="r5">Concatenate</label>
    <input type="radio" name="rad" id="r6" value="r6"><label for="r6">replace</label><br>
    <p></p>
    <button type="button" name="btn" id="btn1" onclick="getresult()">Submit Querry</button>

</body>

</html>
