Problem 14:
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <input type="number" id="rad" name="rad" />
    <label for="rad">Enter radius</label>

    <br />
    <input type="number" id="len" name="len" />
    <label for="len">Enter length</label>
    <input type="number" id="br" name="br" />
    <label for="len">Enter breadth</label>
    <button type="button" id="btn" name="rectbtn" onclick="showRectDetails()">
      Send details
    </button>

    <div id="mydiv"></div>
  </body>
  <script>
    //rect obj
    const rect = {
      len: document.getElementById("len").value,
      br: document.getElementById("br").value,
    };

    //circle obj
    const cir = { rad: document.getElementById("rad").value };

    function showRectDetails() {
      var area = calcArea(rect);
      var pt = peri(rect);

      var areac = areaOfCircle(cir);
      var periOfCircle = periC(cir);
      document.getElementById("mydiv").innerHTML +=
        "Area of rect:" + area + " " + pt;
      document.getElementById("mydiv").innerHTML +=
        "Area of circle:" + areac + " " + periOfCircle;
    }

    function calcArea(rect) {
      var area = rect.len * rect.br;
      return area;
    }

    function peri(rect) {
      var temp = 2 * (rect.len + rect.br);
      return temp;
    }

    //circle

    function areaOfCircle(cir) {
      var area = Math.PI * cir.rad * cir.rad;
      return area;
    }

    function periC(cir) {
      var peri = 2 * Math.PI * cir.rad;
      return peri;
    }
  </script>
</html>

 
________________________________________
